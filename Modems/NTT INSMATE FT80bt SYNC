!! "INSMATE FT80bt SYNC 5/29/01"!@ORIGINATE@ANSWER!! set up the serial port!serreset 115200, 0, 8, 1!! reset the serial port!hsreset 0 0 0 0 0 0settries 0!@LABEL 1!! set up the terminal adapter!matchclrmatchstr 1 2 "OK\13\10"!! E0    - turn command echo off! Q0V1X3- return detailed result code !                                                         pause 1write "ATE0Q0V1\13"matchread 30inctriesiftries 3 59jump 1!@LABEL 2matchclrmatchstr 1 3 "OK\13\10"!! &D1  Å@ - regard DTR as ON! $N11=0  - set 64KPPP mode! S0=1 Å@ - answer on first ring!pause 1write "AT&D0$N11=0S0=0$N9=10\13"matchread 30inctriesiftries 2 59!DTRclearpause 5DTRsetflushjump 2!! TA(terminal adapter) ready,so enable answering,or originate a call!@LABEL 3note "INSMATE FT80bt Ready"3ifANSWER 30!!! @ORIGINATE!!@LABEL 10matchclrmatchstr  1 11 "CONNECT\13\10"matchstr  2 32 "RING\13\10"matchstr  3 50 "NO CARRIER\13\10"matchstr  4 51 "ERROR\13\10"matchstr  5 53 "BUSY\13\10"!note "Dialing ^1" 3pause 1write "ATD^1\13"matchread 700jump 59!@LABEL 11note "Communicating at 64000 Kbps" 3jump 25!@LABEL 25! turn on CTS handshakinghsreset 0 1 0 0 0 0!@LABEL 26exit 0!!! @ANSWER!!@LABEL 30ifORIGINATE 10!@LABEL 31matchclrmatchstr  1 11 "CONNECT\13\10"matchstr  2 32 "RING\13\10"!matchread 700jump 31!!@LABEL 32userhook 1jump 31!!! error messages!!! NO CARRIER@LABEL 50exit -6021!! ERROR@LABEL 51exit -6016!! BUSY@LABEL 53exit -6022!! TA Not Responding @LABEL 59exit -6019!!! Hang up the TA!!@HANGUP@LABEL 60settries 0hsreset 0 0 0 0 0 0!@LABEL 61!! try to get control of the TA!pause 15write "+++"pause 15!matchclrmatchstr 1 63 "NO CARRIER\13\10"matchstr 2 63 "OK\13\10"matchstr 3 63 "ERROR\13\10"write "ATH\13"matchread 30inctriesiftries 2 59!DTRclearpause 5DTRsetflushjump 61!@LABEL 63pause 15matchclrmatchstr 1 65 "OK\13\10"pause 1write "ATZ0\13"matchread 30jump 59!@LABEL 65exit 0!